{
    "name": "nzo/json-schema-validator",
    "type": "symfony-bundle",
    "description": "Json Schema Validation for PSR7 & PSR17",
    "keywords": ["openapi", "http", "validation", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ala Eddine Khefifi",
            "email": "alakhefifi@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "league/openapi-psr7-validator": "^0.17",
        "nyholm/psr7": "^1.0",
        "psr/http-message": "^1.0",
        "symfony/framework-bundle": "^5.0 || ^6.0",
        "symfony/http-foundation": "^5.0 || ^6.0",
        "symfony/psr-http-message-bridge": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Nzo\\JsonSchemaValidatorBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    }
}
